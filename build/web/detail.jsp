            <div class="sidebar-products clearfix">
              <h2>Bestsellers</h2>
            <c:forEach items="${listSaleP}" var="s">
              <div class="item">
                <a href="detail?id=${s.getProductID()}"><img src="${s.getImageURL()}" alt="${s.getName()}"></a>
                <h3><a href="detail?id=${s.getProductID()}">${s.getName()}</a></h3>
                <div class="price">${s.getPrice()} $</div>
              </div>
          
              </c:forEach>
            </div>
          </div>
          <!-- END SIDEBAR -->

          <!-- BEGIN CONTENT -->
          <div class="col-md-9 col-sm-7">
            <div class="product-page">
            
                    <div class="row">
                <div class="col-md-6 col-sm-6">
                  <div class="product-main-image">
                    <img src="${d.getImageURL()}" alt="Cool green dress with red bell" class="img-responsive" data-BigImgsrc="${d.getImageURL()}">
                  </div>
                  
                </div>
                <div class="col-md-6 col-sm-6">
                  <h1>${d.getName()}</h1>
                  <div class="price-availability-block clearfix">
                    <div class="price">
                      <strong><span>$</span> ${d.getPrice() - (d.getPrice() * (d.getSale())/100)}</strong>
                      <c:if test="${d.getSale() > 0}"><em>$<span>${d.getPrice()}</span></em></c:if>
                    </div>
                      <c:if test="${d. getStatus() == 1}">
                    <div class="availability">
                        <h2 style="color: red"> Availability: <strong>In Stock</strong> </h2>
                    </div>
                      </c:if>
                      <c:if test="${d. getStatus() == 0}">
                    <div class="availability">
                        <h4 style="color: red"> Availability: <strong>Out Stock</strong></h4>
                    </div>
                      </c:if>
                  </div>
                  <div class="description">
                    <p>${d.getDescription() }</p>
                  </div>
                
                  <div class="product-page-cart">
                      <form name="f" method="get" action="buy">
                    <div class="product-quantity">
                         <input type="number" name="num"  value=1 min="1" max="20">
                    </div>
                      <input type="number" name="idProduct" value="${d.getProductID()}" hidden>
                                                 <button class="btn btn-primary" type="submit">Add to cart</button>

                                            </form>
                                             
                                                       
                                                   
                                                
                                                
                  
                  </div>
                  
                 
                </div>

                <div class="product-page-content">
                  <ul id="myTab" class="nav nav-tabs">
                    <li><a href="#Description" data-toggle="tab">Description</a></li>
                    
                    <li class="active"><a href="#Reviews" data-toggle="tab">Reviews (2)</a></li>
                  </ul>
                  <div id="myTabContent" class="tab-content">
                    <div class="tab-pane fade" id="Description">
                      <p>${d.getDescription() } </p>
                    </div>
                   
                    <div class="tab-pane fade in active" id="Reviews">
                      <!--<p>There are no reviews for this product.</p>-->
